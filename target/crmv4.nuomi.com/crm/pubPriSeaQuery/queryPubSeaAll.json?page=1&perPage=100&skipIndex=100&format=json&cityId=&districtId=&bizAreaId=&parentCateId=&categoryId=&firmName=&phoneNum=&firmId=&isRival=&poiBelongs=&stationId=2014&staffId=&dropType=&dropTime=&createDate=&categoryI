<script type="text/javascript"> 
   $(function(){
	   getRedis();
         var interva = setInterval(getRedis, 600000);
	    $("#icon").toggle(function(){
			$("#extra_top_menu").stop().animate({left:"530px"},1000);
			$("#icon").attr("class","icon-chevron-left");
			$(".hidebar").hide();
			$("#username").hide();
			$(".devider").hide();
			$(".fixmenu").css("width","35px");
			$("#icon").attr("title","展开");
		},function(){
		 $("#extra_top_menu").stop().animate({left:"0px"},1000);
		 $(".fixmenu").css("width","80%");
		 		$(".hidebar").show();
		 		$("#username").show();
		 		$(".devider").show();
		 		$("#icon").attr("class","icon-chevron-right");
		 		$("#icon").attr("title","收起");
		});
	var $list = $(".list");		
	var speed =3000;		
	var sto = setTimeout(function(){
		autoScroll();
	},speed);
	$list.hover(function(){
		clearTimeout(sto);
	},function(){
		sto = setTimeout(function(){autoScroll();},speed);
	});
	function autoScroll(){ 
	    $list.css('margin-top','-25px');
	    setTimeout(function(){
	    	$list.css('margin-top','0px').find("li:first").appendTo($list);
	    }, 1000);
	    sto = setTimeout(function(){autoScroll();},speed);
	} 
}); 
     function getRedis() { 
           	 $.get("/arret/getRedisArret", function(data){
           	   var html ='';	
	     		for(var i=0;i<data.length;i++){
		     		var id=data[i].id;
		     		var title=data[i].title;
		     		var content=data[i].content;
		     		html += " <li id='"+id+"' onclick='getdialog(this);' style='overflow:hidden;' >"+title+"</li> ";
	     		}
	     		$(".list").html(html);
     		},'json');	
           }  
   function getdialog(obj){
	var id=$(obj).attr("id");
		$.fancybox.showActivity();
	$.fancybox({
			'width'				: 700,
			'height'			: 400,
			'scrolling'         : 'auto',
			'autoDimensions'	: true,
			'autoScale'			: false,
			'transitionIn'		: 'none',
			'transitionOut'		: 'none',
			'type'				: 'iframe',
			'href':'/arret/dialogArret?id='+id
	});
	}

</script> 
