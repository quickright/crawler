    <script type="text/javascript">
        var map = new Map();
        var openMenu = function (obj, url) {
            var vue = map.get(obj.id);
            if (null == vue) {
                map.put(obj.id, true);
                openIframe(obj.id, url);
                $(".current").removeClass('current');
                $(obj).addClass('current');
                setTimeout(function () {
                    map.remove(obj.id);
                }, 1000 * 2);
            }
        }

        var openIframe = function (id, url) {
            if (url != '') {
                if (url.indexOf('?') != -1)
                    $('#iframe').attr('src', url + "&date=" + new Date().getTime() + "&funcPlatformId=" + id);
                else
                    $('#iframe').attr('src', url + "?date=" + new Date().getTime() + "&funcPlatformId=" + id);
            }
        }
        var toggleMenu = function () {
            $('.opet-title').toggle(function () {
                $(this).addClass('title-expanded').next().slideDown();
            }, function () {
                var me = this;
                $(this).next().slideUp(function () {
                    $(me).removeClass('title-expanded');
                });
            });

            $('.opet-btn ul li').hover(function () {
                $(this).addClass('hover');
            }, function () {
                $(this).removeClass('hover');
            })
        }
        var logout = function () {
            window.location.href = baseUrl + "/login/out";
        }

    </script>
    <script type="text/javascript">

        function init(iframeId) {
            var real_location = '';
            if (getCookie('toIndex') == 'true') {
                real_location = getCookie('indexUrl');
            } else {
                real_location = getCookie('iframeUrl');
                if (real_location == null || real_location == '') {
                    real_location = 'http://sso.nuomi.com/portal';
                }

                if (real_location == null || real_location == '') {
                    real_location = getCookie('indexUrl');
                }
            }
            if(real_location==='http://sso.nuomi.com/'){
                real_location='http://sso.nuomi.com/portal';
            }
            $('#iframe').attr('src', real_location);

            var Sys = {};
            var ua = navigator.userAgent.toLowerCase();
            if (window.ActiveXObject)
                Sys.ie = ua.match(/msie ([\d.]+)/)[1]
            if (Sys.ie) {
                document.getElementById(iframeId).attachEvent("onload", toTop);
            }
            else {
                document.getElementById(iframeId).onload = toTop;
            }
        }

        function toTop() {
            document.getElementById("iframe").height = 400;
            document.documentElement.scrollTop = 0;
            document.body.scrollTop = 0;
        }


        function reinitIframe() {

            var iframe = document.getElementById("iframe");
            try {

                var bHeight = iframe.contentWindow.document.body.scrollHeight;
                var dHeight = iframe.contentWindow.document.documentElement.scrollHeight;
                var height = Math.max(bHeight, dHeight);
                iframe.height = height;
            } catch (ex) {
                iframe.height = document.getElementById('bd').scrollHeight - 5;
            }

        }


    </script>
    <script>
        function switchUser(user_id, url) {

            refresh_flg = false;

            if (user_id == null) {
                window.location.href = "/user/switchUser";
            } else {
                window.location.href = baseUrl + '/agent/switchUser?userId=' + user_id + '&url=' + url + '&date=' + new Date().getTime();
            }
        }
    </script>


    <script>
        var index_flg = false;
        var refresh_flg = true;

        function goodbye() {

            if (index_flg) {
                var index_loc = getCookie('indexUrl');
                window.location.href = baseUrl + '/index/index?remeberURL=' + index_loc;
                setCookie('toIndex', 'true');

            } else if (refresh_flg) {
                var loc = document.getElementById('iframe').contentWindow.location;
                if (loc != null && loc != '') {
                    window.location.href = baseUrl + '/index/index?remeberURL=' + loc;
                    setCookie('iframeUrl', loc);
                    setCookie('toIndex', 'false');
                }
            }

        }

        function goToIndexPage() {

            index_flg = true;
            goodbye();

        }

        function goToLogout() {
            refresh_flg = false;

            window.location.href = baseUrl + '/user/logout';
        }
        function setCookie(c_name, value, expiredays) {
            var exdate = new Date()
            exdate.setDate(exdate.getDate() + expiredays)
            document.cookie = c_name + "=" + escape(value) + ";domain=.nuomi.com;path=/" +
                    ((expiredays == null) ? "" : ";expires=" + exdate.toGMTString())
        }

        function getCookie(c_name) {
            if (document.cookie.length > 0) {
                c_start = document.cookie.indexOf(c_name + "=")
                if (c_start != -1) {
                    c_start = c_start + c_name.length + 1
                    c_end = document.cookie.indexOf(";", c_start)
                    if (c_end == -1) c_end = document.cookie.length
                    return unescape(document.cookie.substring(c_start, c_end))
                }
            }
            return ""
        }

        var baseline = 1;
        $(document).ready(function () {
            baseline = new Date().getTime();
            /* 待办信息 */
            var loadPreCount = function () {
                var now = new Date();
                $.ajax({
                    type: "GET",
                    url: '/ticket/loadPreCount?t=' + now.getTime() + '&baseline=' + baseline,
                    data: {},
                    dataType: "json",
                    success: function (v) {
                        var totalCounts = v.totalCounts;
                        if (totalCounts > 99) {
                            $('#tips .qqonline_sub').first().html('<b>9+</b>');
                        }
                        else if (totalCounts > 0) {
                            $('#tips .qqonline_sub').first().html('<b>' + totalCounts + '</b>');
                        }
                        else {
                            $('#tips .qqonline_sub').first().html('<b>0</b>');
                        }
                    },
                    error: function (req, msg, ex) {
                        $('#tips .qqonline_sub').first().html('<b>0</b>');
                    }
                });
            };
// 		loadPreCount();
// 		setInterval(loadPreCount, 120000);

            $("#tips").fancybox({
                'width': '80%',
                'height': '80%',
                'scrolling': 'auto',
                'autoDimensions': true,
                'autoScale': false,
                'transitionIn': 'none',
                'transitionOut': 'none',
                'type': 'iframe'
            });
            var tips;
            var theTop = 100/*这是默认高度,越大越往下*/;
            var old = theTop;

            function initFloatTips() {
                tips = document.getElementById('divQQbox');
                moveTips();
            };
            function moveTips() {
                var tt = 50;
                if (window.innerHeight) {
                    pos = window.pageYOffset
                } else if (document.documentElement && document.documentElement.scrollTop) {
                    pos = document.documentElement.scrollTop
                } else if (document.body) {
                    pos = document.body.scrollTop;
                }
                pos = pos - tips.offsetTop + theTop;
                pos = tips.offsetTop + pos / 10;
                if (pos < theTop) pos = theTop;
                if (pos != old) {
                    tips.style.top = pos + "px";
                    tt = 10;
                }
                old = pos;
                setTimeout(moveTips, tt);
            }

            initFloatTips();
        });
    </script>

